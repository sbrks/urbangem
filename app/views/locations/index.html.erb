<div class="container-fluid">
	<h2>Location Feed</h2>
	<% @locations.each do |location| %>
		<div class="row">
			<div class="col-md-12 location">
				<div class="col-md-4">
				<img src="<%= location.img_url %>" width="300px" height="300px">
				</div>
				<div class="col-md-8">
					<h2><%= location.name %></h2>
					<h4>
						Address: <%= location.address %>
					</h4>
				</div>
				<% end %>
			</div>
		</div>
	</div>
